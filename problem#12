#include<iostream>
#include<string>
using namespace std;
void Readnumpers(int& num1, int& num2)
{
	cout << "pless Enter num1" << endl;
	cin >> num1;
	cout << "pless Enter num2" << endl;
	cin >> num2;

}
int max2numpers(int num1, int num2)
{
	if (num1 > num2)
		return num1;
	else
		return  num2;
}
void printReseltmax(int max)
{
	cout << "Themax=" << max << endl;
}
int main()
{
	int num1, num2;
	Readnumpers(num1, num2);
	printReseltmax(max2numpers(num1, num2));
	return 0;
}
