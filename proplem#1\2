#include<iostream>
#include<string>
using namespace std;
void printtypelHuder()
{

	cout << "            malte    taypl  1    to    10  " << endl;


	for (int i = 1; i <= 10; i++)
	{

		cout << i << "       ";

	}
	cout << endl;
	cout << "____________________________________________________________________________________________________________________";
	cout << endl;
	for (int i = 1; i <= 10; i++)
	{

		cout << i << " " << "|" << i << "    " << i * 2 << "      " << i * 3 << "      " << i * 4 << "        " << i * 5 << "         " << i * 6 << "       " << i * 7 << "        " << i * 8 << "        " << i * 9 << "         " << i * 10 << endl;
	}

}


	int main()
	{




		printtypelHuder();



		return 0;

	}
