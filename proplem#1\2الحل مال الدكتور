#include<iostream>
#include<string>
using namespace std;
void printtypelHuder()
{
	cout << "\n\n\t\t\t mlte type 1 to 10\n\n";
	cout << "\t";
	for (int i = 1; i <= 10; i++)
	{
		cout << i << "\t";
	}
	cout << "\n ______________________________________________________________________________________\n";
}
string columsport(int i)
{
	if (i < 10)

		return "    |";
	else
		return"   |";


}
void printtype()
{
	printtypelHuder();
	for (int i = 1; i <= 10; i++)
	{
		cout << "" << i << columsport(i) << "\t";
		for (int j = 1; j <= 10; j++)
		{
			cout << i * j << "\t";
		}
		cout << endl;
	}
	

}
int main()
{


	printtype();


	return 0;
}
