}
float  ReadLanAmount(float LanAmoument, float motheds)
{

	return (float)LanAmoument / motheds;

}
int main()
{
	float LanAmoument = Readpositivenumper("pless Enter LanAmoument");
	float motheds = Readpositivenumper("pless Enter motheds");
	cout << ReadLanAmount(LanAmoument, motheds);
	cout << endl;



